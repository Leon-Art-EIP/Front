{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    { "src": "web-app-folder", "use": "@vercel/node" },
    { "src": "storybook-folder", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/web-app/(.*)", "dest": "web-app-folder/$1" },
    { "src": "/storybook/(.*)", "dest": "storybook-folder/$1" }
  ],
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "./web-app-folder"
}
